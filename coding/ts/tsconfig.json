{
    "compilerOptions": {
        "target": "ESNEXT",
        "module": "ESNext",
        "lib": [
            "lib.esnext.d.ts"
        ],
        "noEmit": false,
        "strict": true,
        "typeRoots": [
            "../node_modules/@types"
        ],
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "node_modules",
        ".build"
    ]
}